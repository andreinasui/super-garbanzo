{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "printWidth": 80,
  "singleQuote": true,
  "semi": true,
  "trailingComma": "es5",
  "importOrder": [
    "^@(.*)/(.*)$",
    "^api",
    "^dto",
    "^pages",
    "^components",
    "^constants",
    "^configs",
    "^hooks",
    "^utils",
    "^assets",
    "^contexts",
    "^[./]",
    ".scss"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
